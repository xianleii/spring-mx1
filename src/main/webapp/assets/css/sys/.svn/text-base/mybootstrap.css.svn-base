.table {
	font-size: 14px;
	margin-bottom: 10px;
}

.table>tbody>tr>td {
	color: #666666;
	table-layout: fixed;
	word-wrap:break-word;
	word-break: break-all;
}
.table > thead > tr > th, .table > tbody > tr > th, .table > tfoot > tr > th, .table > thead > tr > td, .table > tbody > tr > td, .table > tfoot > tr > td {
	padding: 4px;
}
.table>thead>tr>th {
	font-weight: normal;
	color: #333333;
	min-width: 70px;
	word-wrap:break-word;
	word-break: break-all;
	padding: 6px 2px;
}

.form-horizontal .control-label {
	color: #333333;
}

